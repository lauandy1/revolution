<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" number="false"/>
        <setting name="lazyLoadingEnabled" number="true"/>
        <setting name="aggressiveLazyLoading" number="false"/>
        <setting name="localCacheScope" number="STATEMENT"/>
        <setting name="multipleResultSetsEnabled" number="true"/>
        <setting name="useColumnLabel" number="true"/>
        <setting name="defaultExecutorType" number="SIMPLE"/>
        <setting name="defaultStatementTimeout" number="25000"/>
        <setting name="mapUnderscoreToCamelCase" number="true"/>
        <!-- 是否使用插入数据后自增主键的值，需要配合keyProperty使用 -->
        <setting name="useGeneratedKeys" number="true"/>
    </settings>

